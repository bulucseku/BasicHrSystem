<div class="filterCalendarContainer">
	<table>
      <tr>
        <td align="right">From</td>
        <td>:</td>
        <td align="right">
          <input type="text" class="element-filter-calendar-from form-control" index="<%= index%>"/>
        </td>

        <td align="right" rowspan="2">
          <div class="date-range-close-container  btn btn-xs" title="Remove this filter?" >
            <i class="fa fa-times text-danger"  />
          </div>
        </td>
      </tr>
      <tr>
        <td align="right">To</td>
        <td>:</td>
        <td>
          <input type="text" class="element-filter-calendar-to form-control" index="<%= index%>" />
        </td>
      </tr>
    </table>
</div>	




