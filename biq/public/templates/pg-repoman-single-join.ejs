<tr class="<%= id%>">
	<td style="padding: 10px;">
		Table Join
	</td>
	<td style="padding: 10px;">
		<input type="text" class="input-name" />
	</td>
	<td style="padding: 10px;">
		<select class="select-join-type">
			<option value="0">Inner</option>
			<option value="1">Left</option>
			<option value="2">Right</option>
			<option value="3">Full</option>
		</select>
	</td>
	<td style="padding: 10px;">
		<select class="select-comparison-type">
			<option value="0">Equals</option>
			<option value="1">GreaterThan</option>
			<option value="2">GreaterThanOrEquals</option>
			<option value="3">LessThan</option>
			<option value="4">LessThanOrEquals</option>
			<option value="5">Contains</option>
			<option value="6">NotContain</option>
			<option value="New">New</option>
		</select>
	</td>	
	<td style="padding: 10px;">
		<input type="checkbox" class="checkbox-literal" />
	</td>
	<td style="padding: 10px;">
		<input type="text" class="input-database1" />
	</td>
	<td style="padding: 10px;">
		<input type="text" class="input-database2" />
	</td>
	<td style="padding: 10px;">
		<div class="button-add-segments">Set Segments</div>
	</td>
	<td style="padding: 10px;">
		<div class="button-root-filter">Root Filter</div>
	</td>
</tr>
