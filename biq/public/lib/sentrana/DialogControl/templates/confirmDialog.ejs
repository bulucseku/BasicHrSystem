<div class="modal fade sentrana-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">
          <span aria-hidden="true">&times;</span>
          <span class="sr-only">Close</span>
        </button>
        <h4 class="modal-title"><%= title %></h4>
      </div>
      <div class="modal-body">
          <% if (messageType) { %>
              <span class='sprite_alert_icon
                <% if (messageType === 'Error') { %> <%= 'sprite_error_icon' %>
                <% } else if (messageType === 'Warning') { %> <%= 'sprite_warning_icon' %>
                <% } else if (messageType === 'Stop') { %> <%= 'sprite_stop_icon' %>
                <% } else if (messageType === 'Confirmation') { %> <%= 'sprite_confirm_icon' %>
                <% } else if (messageType === 'Information') { %> <%= 'sprite_alert_info_icon' %>
                <% } else if (messageType === 'Question') { %> <%= 'sprite_question_icon' %>
                <% } %>'
                    style='float: left; margin: 0 7px 20px 0;'>
              </span>
          <% } %>
          <p class="confirm-message"><%== message %></p>
      </div>
      <div class="modal-footer">
        <div class="status-bar">
          <span class="status-bar-loading" style="display: none;"></span>
            <span class="status-bar-msg"></span>
        </div>
        <div class="action-buttons">
          <% can.each(buttons, function(button, index) { %>
            <button type="button" class="btn <%= button.className %>" id="<%= button.id %>">
              <%= button.label %>
            </button>
          <% }); %>
        </div>
      </div>
    </div>
  </div>
</div>
