<div class="property-window">
      <table class="tbl-property-window">
        <tr>
          <td>Fore color</td>
          <td>
            <input id="page-item-foreColor" type="text" value="<%= model.foreColor%>">
              
            </input>
          </td>
        </tr>
        <tr>
          <td>Back Color</td>
          <td>
            <input id="page-item-backColor" type="text" value="<%= model.backColor%>"></input>
          </td>
        </tr>
        <tr>
          <td>Edit Mode</td>
          <td>
            <input id="editMode" type="checkbox" checked="<%= model.isEditMode%>"></input>
          </td>
        </tr>
      </table>
    </div>
