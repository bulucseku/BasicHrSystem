<ul id="nav-page-list" class="nav navbar-nav">
    <% can.each(navItems, function(navItem, key) { %>
        <% if (navItem.children && navItem.children.length > 0) { %>
            <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><%= navItem.btnName %><span class="caret"></span></a>

                <ul class="dropdown-menu" role="menu">
                    <% can.each(navItem.children, function(childItem, key) { %>
                        <li class="nav-page-link">
                        <a href="#!<%= childItem.btnID %>"><%= childItem.btnName %></a></li>
                    <% }) %>
                </ul>

        <% } else { %>
            <li class="nav-page-link">
            <a href="#!<%= navItem.btnID %>"><%= navItem.btnName %></a>
        </li>
        <% } %>
    <% }) %>
</ul>